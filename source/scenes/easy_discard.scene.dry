title: Вернуться назад
go-to: root
on-arrival: {!
Q.week_actions -= 1;
// return card to hand
var card = this.state.lastPlayedCard;
card.image = card.cardImage;
this.state.currentHands['main'].push(card);
// reset the timer...
if (Q[card.id + "_timer"]) {
    Q[card.id + "_timer"] = 0;
}
// reduce visit count
if (this.state.visits[card.id]) {
    this.state.visits[card.id] -= 1;
}
!}
frequency: 111111111111111111111111111111111111111
